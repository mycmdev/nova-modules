{
    "name": "mycmdev/nova-modules",
    "description": "A Laravel Nova Tool helping modularize your application",
    "keywords": [
        "laravel",
        "nova",
        "cmdev",
        "module",
        "generator"
    ],
    "authors": [
        {
            "name": "Claudio Castorina",
            "email": "claudio.castorina01@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.3.15"
    },
    "require-dev": {
        "laravel/framework": "^7.0",
        "laravel/nova": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Cmdev\\NovaModules\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cmdev\\NovaModules\\NovaModulesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
